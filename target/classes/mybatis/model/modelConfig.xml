<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- board -->
		<typeAlias type="com.spring.Uhdiya.board.notice.NoticeDTO" alias="noticeDTO"/>
		<typeAlias type="com.spring.Uhdiya.board.notice.NoticeFileDTO" alias="noticeFileDTO"/>
		<typeAlias type="com.spring.Uhdiya.board.qna.QnaDTO" alias="qnaDTO"/>
		<typeAlias type="com.spring.Uhdiya.board.qna.QnaFileDTO" alias="qnaFileDTO"/>
		<typeAlias type="com.spring.Uhdiya.board.review.ReviewDTO" alias="reviewDTO"/>
		<typeAlias type="com.spring.Uhdiya.board.review.ReviewFileDTO" alias="reviewFileDTO"/>
		
		<!-- member -->
		<typeAlias type="com.spring.Uhdiya.member.MemberDTO" alias="memberDTO"/>
		<!-- mem -->
		<typeAlias type="com.spring.Uhdiya.mem.MemberBean" alias="memberBean"/>
		<!-- memJ -->
		<typeAlias type="com.spring.Uhdiya.memJ.MemJDTO" alias="memJDTO"/>
		
		<!-- shoppingList -->
		<typeAlias type="com.spring.Uhdiya.cart.CartDTO" alias="cartDTO" />
		<typeAlias type="com.spring.Uhdiya.cart.CartListDTO" alias="cartListDTO" />
		<typeAlias type="com.spring.Uhdiya.order.OrderDTO" alias="orderDTO" />
		
		<!-- product -->
		<typeAlias type="com.spring.Uhdiya.product.ProductDTO" alias="productDTO" />
		<typeAlias type="com.spring.Uhdiya.product.ProductFileDTO" alias="productFileDTO" />
		<typeAlias type="com.spring.Uhdiya.product.ProductCntDTO" alias="productCntDTO" />
	</typeAliases>
</configuration>